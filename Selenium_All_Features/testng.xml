<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- How to run in parallel from Test NG -->
<suite name="Suite" parallel="tests" thread-count="5">


	<test thread-count="5" name="Class1 Tests">

		<classes>
			<!-- package.classname -->
			<class name="testNG_4.Class1">
			
			<methods>
			
			<exclude name = "testMethod1"/>
			
			 </methods>
			 
			 </class>
		
		</classes>
	</test> <!-- Test -->



	<test thread-count="5" name="Class2 Tests">

		<classes>
			<!-- package.classname -->
			
			<class name="testNG_4.Class2" />
		</classes>
	</test> <!-- Test -->
	
	
</suite> <!-- Suite -->
